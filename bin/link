ln -sfn ~/dotfiles/vim ~/.vim
ln -sfn ~/dotfiles/ctags.d ~/.ctags.d
ln -fs ~/dotfiles/vim/vimrc ~/.vimrc
ln -fs ~/dotfiles/vim/gvimrc ~/.gvimrc
ln -fs ~/dotfiles/vim/nvimrc ~/.nvimrc
mkdir -p ~/.config/nvim
ln -fs ~/dotfiles/nvim/init.vim ~/.config/nvim/init.vim
ln -fs ~/dotfiles/eslintrc.json ~/.eslintrc.json
